function DisplayTimeTable(Batch: string):void
{
switch(Batch)
{
    case "PPA":
    {
     console.log("Batch timing: 1PM")
     break
    }
    case "Angular":
    {
        console.log("Batch timing: 4PM")
        break
    }
    case "LB":
    {
        console.log("Batch timing: 7.30PM")
        break

    }
    default :
    {
        console.log("There is no such Batch")
    }  
}
}
DisplayTimeTable("LB")
