var Data : number[] = new Array(4)
Data = [11,21,51,101]

var Cnt : number = 0
var addition : number = 0

for(Cnt=0 ; Cnt<Data.length ;Cnt++)
{
    console.log("Array element of [Cnt] is:"+Data[Cnt])
}
for(Cnt=0 ; Cnt<Data.length ;Cnt++)
    {
       
        addition = addition + Data[Cnt]
        
    }
    console.log("Summation element is:"+addition)
