//Characteristics - Data
//Behaviours  -Function
class Arithmetic
{
  // Chracteristics
  No1 : number
  No2 : number

  //Behaviours
  constructor (No1 : number , No2 : number)
  {
   this.No1 = No1
   this.No2 = No2
  }
  Addition() : number
  {
    var Ans : number = 0;
    Ans = this.No1 + this.No2
    return Ans
  }
  Substraction() : number
  {
    var Ans : number = 0;
    Ans = this.No1 - this.No2
    return Ans
  }
}
var obj1 = new Arithmetic(11,10)

var Result : number = 0

Result = obj1.Addition()
console.log("Addition is" + Result)

Result = obj1.Substraction()
console.log("Substraction is" + Result)

var obj2 = new Arithmetic(51,42)

var Result : number = 0

Result = obj2.Addition()
console.log("Addition is" + Result)

Result = obj2.Substraction()
console.log("Substraction is" + Result)
